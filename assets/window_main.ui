<?xml version="1.0" encoding="UTF-8"?>
<interface>
	<menu id="appmenu">
		<section>
			<item>
				<attribute name="label">Preferences</attribute>
			</item>
		</section>
		<section>
			<item>
				<attribute name="label">Quit</attribute>
			</item>
		</section>
	</menu>
	<object class="GtkWindow" id="GtkMainWindow">
		<property name="window_position">center</property>
		<property name="default_width">800</property>
		<property name="default_height">500</property>
		<child type="titlebar">
			<object class="GtkHeaderBar" id="headerBar">
				<property name="title">gTorrent</property>
				<property name="show_close_button">True</property>
				<child>
					<object class="GtkButton" id="addTorrentButton">
						<style>
							<class name="button"/>
							<class name="image-button"/>
						</style>
						<child>
							<object class="GtkImage" id="addTorrentImage">
								<property name="icon_size">1</property>
								<property name="icon_name">list-add-symbolic</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkMenuButton" id="addMagnetButton">
						<property name="use_popover">True</property>
						<style>
							<class name="button"/>
							<class name="image-button"/>
						</style>
						<child>
							<object class="GtkImage" id="addMagnetImage">
								<property name="icon_size">1</property>
								<property name="icon_name">insert-link-symbolic</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkMenuButton" id="addButtonRss">
						<style>
							<class name="button"/>
							<class name="image-button"/>
						</style>
						<child>
							<object class="GtkImage">
								<property name="icon_size">1</property>
								<property name="icon_name">preferences-system-symbolic</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkSeparator" id="vSepOne">
						<property name="orientation">GTK_ORIENTATION_VERTICAL</property>
					</object>
					<packing>
						<property name="pack_type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkButton" id="resumeButton">
						<style>
							<class name="button"/>
							<class name="image-button"/>
						</style>
						<child>
							<object class="GtkImage" id="resumeImage">
								<property name="icon_size">1</property>
								<property name="icon_name">media-playback-start-symbolic</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkButton" id="pauseButton">
						<style>
							<class name="button"/>
							<class name="image-button"/>
						</style>
						<child>
							<object class="GtkImage" id="pauseImage">
								<property name="icon_size">1</property>
								<property name="icon_name">media-playback-pause-symbolic</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkButton" id="deleteButton">
						<style>
							<class name="button"/>
							<class name="image-button"/>
						</style>
						<child>
							<object class="GtkImage" id="deleteImage">
								<property name="icon_size">1</property>
								<property name="icon_name">edit-delete-symbolic</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkSeparator" id="vSepTwo">
						<property name="orientation">GTK_ORIENTATION_VERTICAL</property>
					</object>
					<packing>
						<property name="pack_type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkButton" id="preferencesButton">
						<style>
							<class name="button"/>
							<class name="image-button"/>
						</style>
						<child>
							<object class="GtkImage" id="preferencesImage">
								<property name="icon_size">1</property>
								<property name="icon_name">preferences-system-symbolic</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">start</property>
					</packing>
				</child>
				<child>
					<object class="GtkButton" id="settingsButton">
						<style>
							<class name="button"/>
							<class name="image-button"/>
						</style>
						<child>
							<object class="GtkImage" id="settingsImage">
								<property name="icon_size">1</property>
								<property name="icon_name">emblem-system-symbolic</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">end</property>
					</packing>
				</child>
				<child>
					<object class="GtkButton" id="searchButton">
						<style>
							<class name="button"/>
							<class name="image-button"/>
						</style>
						<child>
							<object class="GtkImage" id="searchImage">
								<property name="icon_size">1</property>
								<property name="icon_name">preferences-system-search-symbolic</property>
							</object>
						</child>
					</object>
					<packing>
						<property name="pack_type">end</property>
					</packing>
				</child>
			</object>
		</child>
		<child>
			<object class="GtkBox" id="content">
				<child>
					<object class="GtkRevealer" id="sidebarRev">
						<property name="transition_type">slide-right</property>
						<property name="transition_duration">500</property>
						<style>
							<class name="sidebar"></class>
						</style>
						<child>
							<object class="GtkScrolledWindow" id="sidebarscroll">
								<child>
									<object class="GtkTreeView" id="sidebar">
									</object>
								</child>
							</object>
						</child>
					</object>
				</child>
				<child>
					<object class="GtkPaned" id="panel">
						<property name="orientation">GTK_ORIENTATION_VERTICAL</property>
						<child>
							<object class="GtkBox" id ="torrentbox">
								<property name="orientation">GTK_ORIENTATION_VERTICAL</property>
								<child>
									<object class="GtkScrolledWindow" id="scrolledWindow">
										<child>
											<object class="GtkStack" id="stack_main">
												<child>
													<object class="GtkTreeView" id="treeview_torrent">
														<child>
															<object class="GtkTreeViewColumn" id="col_queue">
																<property name="title" translatable="yes">#</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_name">
																<property name="title" translatable="yes">Name</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_eta">
																<property name="title" translatable="yes">ETA</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_age">
																<property name="title" translatable="yes">Age</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_seeders">
																<property name="title" translatable="yes">Seeders</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_leechers">
																<property name="title" translatable="yes">Leechers</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_upload">
																<property name="title" translatable="yes">Up speed</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_download">
																<property name="title" translatable="yes">Download speed</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_size">
																<property name="title" translatable="yes">Size</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_remaining">
																<property name="title" translatable="yes">Remaining</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_ratio">
																<property name="title" translatable="yes">Ratio</property>
															</object>
														</child>
														<child>
															<object class="GtkTreeViewColumn" id="col_progress">
																<property name="title" translatable="yes">Progress</property>
															</object>
														</child>
													</object>
												</child>
												<child>
													<object class="GtkTreeView" id="treeview_rss">
														<child>
															<object class="GtkTreeViewColumn" id="col_test">
																<property name="title" translatable="yes">TEST</property>
															</object>
														</child>
													</object>
												</child>
											</object>
										</child>
									</object>
								</child>
								<child>
									<object class="GtkSearchBar" id="searchbar">
										<property name="vexpand">False</property>
										<child>
											<object class="GtkSearchEntry" id="searchentry">
											</object>
										</child>
									</object>
								</child>
							</object>
						</child>
						<child>
							<object class="GtkStack" id="stack_info">
							</object>
						</child>
					</object>
				</child>
			</object>
		</child>
	</object>
</interface>
